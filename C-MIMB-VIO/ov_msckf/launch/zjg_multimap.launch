             -0.999933973807000,0.008587162140000,-0.011559553948000 , -0.5712,
           0.011555468451000 ,0.002456450880000,-0.999880708792000 , -0.7248,
            -0.008608003334000,-0.999988929120000,-0.002486110528000 , -0.0056434544,
