        <!-- <param name="path_gt"     type="string" value="$(find ov_data)/euroc_mav/V1_02_medium.txt" /> -->
        <!-- <param name="path_gt"     type="string" value="$(find ov_data)/euroc_mav/V1_03_difficult.txt" /> -->
        <!-- <param name="path_gt"     type="string" value="$(find ov_data)/euroc_mav/V2_01_easy.txt" /> -->
        <!-- <param name="path_gt"     type="string" value="$(find ov_data)/euroc_mav/V2_02_medium.txt" /> -->
        <!-- <param name="path_gt"     type="string" value="$(find ov_data)/euroc_mav/V2_03_difficult.txt" /> -->
        <!-- <param name="path_gt"     type="string" value="$(find ov_data)/euroc_mav/MH_01_easy.txt" /> -->
        <!-- <param name="path_gt"     type="string" value="$(find ov_data)/euroc_mav/MH_02_easy.txt" /> -->
        <!-- <param name="path_gt"     type="string" value="$(find ov_data)/euroc_mav/MH_03_medium.txt" /> -->
        <!-- <param name="path_gt"     type="string" value="$(find ov_data)/euroc_mav/MH_04_difficult.txt" /> -->
        <!-- <param name="path_gt"     type="string" value="$(find ov_data)/euroc_mav/MH_05_difficult.txt" /> -->
        <param name="path_gt"     type="string" value="$(find ov_data)/euroc_mav/MH03_aligned.txt" />



        <!-- match map parameters-->
       <param name="use_prior_map" type="bool" value="$(arg use_prior_map)" />
       <param name="use_multi_map" type="bool" value="$(arg use_multi_map)" />
        <param name="trans_fej" type="bool" value="$(arg trans_fej)" />
        <param name="use_schmidt" type="bool" value="$(arg use_schmidt)" />
        <param name="map_save_path" type="string" value="$(arg map_save_path)" /> 
        
        <param name="num_map" type="int" value="$(arg num_map)" />
        <param name="pose_graph_filename_1" type="string" value="$(arg pose_graph_filename_1)" />
        <param name="keyframe_pose_filename_1" type="string" value="$(arg keyframe_pose_filename_1)" />
        <param name="pose_graph_filename_2" type="string" value="$(arg pose_graph_filename_2)" />
        <param name="keyframe_pose_filename_2" type="string" value="$(arg keyframe_pose_filename_2)" />
        <!-- <param name="pose_graph_filename_3" type="string" value="$(arg pose_graph_filename_3)" />
        <param name="keyframe_pose_filename_3" type="string" value="$(arg keyframe_pose_filename_3)" />
        <param name="pose_graph_filename_4" type="string" value="$(arg pose_graph_filename_4)" />
        <param name="keyframe_pose_filename_4" type="string" value="$(arg keyframe_pose_filename_4)" />  -->
        <param name="load_map_traj" type="bool" value="true"/>
        <param name="querry_pose_filename_1" type="string" value="$(arg querry_pose_filename_1)" />
        <param name="querry_pose_filename_2" type="string" value="$(arg querry_pose_filename_2)" />
        

        <param name="points_filename" type="string" value="/tmp/match_points.txt" />
        <param name="save_match_points" type="bool" value="true" />
        <!-- <param name="match_num_thred" type="int" value="30" /> -->
        <param name="match_num_thred" type="int" value="40" />
        <param name="pixel_error" type="double" value="10.0" />
        <param name="transform_cov" type="double" value="10.0" />
        <param name="keyframe_cov_pos" type="double" value="0.25"/>
        <param name="keyframe_cov_ori" type="double" value="0.0025"/>
