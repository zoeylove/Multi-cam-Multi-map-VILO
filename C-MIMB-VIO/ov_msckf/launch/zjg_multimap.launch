        <rosparam param="cam0_d">[-0.049567815049682294,0.069208648438352491,-0.000048199233985591757,-0.0017606132974436555]</rosparam>
        <rosparam param="cam1_k">[1208.05,1248.73,783.60000000000002,557]</rosparam>
        <rosparam param="cam1_d">[-0.047353881275324715,0.065058204642788983,0.00016046245482810492,0.0021778853984350918]</rosparam>
        <rosparam param="cam2_k">[500.399,516.84299999999996,507.39499999999998,407.89999999999998]</rosparam>
        <rosparam param="cam2_d">[-0.16624276882002614,0.040483275079251431,-0.00059364969933710778,-0.0012091693281652742]</rosparam>
	      <rosparam param="cam3_k">[501.88400000000001,518.01300000000003,531.59699999999998,397.49200000000002]</rosparam>
        <rosparam param="cam3_d">[-0.17181846847240198,0.044251920178279343,0.000085455755388265042,0.0006121597092667994]</rosparam> -->

        <!-- camera extrinsics -->
        <rosparam param="T_C0toI">
            [
            -0.0077,-0.0039,1.0000,0.035,
            -0.9999,-0.0065,-0.0077,-0.025,
            0.0066,-1.000,-0.0038,0.0012,
             0.,          0.,          0,          1.
            ]
        </rosparam> 
        
        <rosparam param="T_C1toI">
            [
           0.001500555959000,  -0.001516710184000, 1.000035170085000, 0.0406,
           -0.999939509041000, -0.004557318517000, 0.001518076031000,-0.5230,
           0.004625530036000, -1.000016942174000, -0.001474439724000,0.003407343440000,
            0.,          0.,          0,          1.
            ]
        </rosparam>  
   
        
        
	    <rosparam param="T_C2toI">
            [
            0.999880781153900,-0.004378747680000,0.017131564870000 , -0.5123,
            -0.017274883771500,-0.042124116438100,0.998913715544000 , 0.2701,
            -0.003602105538000,-0.999129033541400,-0.042265593364000,0.054192668200000,
