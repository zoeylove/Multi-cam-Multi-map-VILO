            -0.00591885806534344,-0.00520039154403561,  0.999968961041787, 0.0726543980762117,
            0.99970722905452,  0.0234304230094901,0.00603916001859865, 0.0363412526123123,
            -0.0234611017502632,   0.999711944114596,0.00506018767228228,-0.0111909429509495,
