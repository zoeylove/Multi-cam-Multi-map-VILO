        <rosparam param="cam3_k">[684.100762539661,684.792850321549,582.511083857304,358.473329685483]</rosparam>
        <rosparam param="cam3_d">[0.0234781926967344,-0.0559150241872289,0,0]</rosparam> -->
        <rosparam param="cam0_k">[680.986566365892,680.73354007274,572.304618977409,355.553433262214]</rosparam>
        <rosparam param="cam0_d">[0.0213550511868105 , -0.0500832912988368, 0.0, 0.0]</rosparam>
